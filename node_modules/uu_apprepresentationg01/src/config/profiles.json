{
  "{asid}": {
    "profileList": ["AsidAuthorities", "AsidExecutives", "Public"],
    "useCaseMap": {
      "sys/uuSubAppInstance/productInfo/set": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities", "AsidExecutives"]
      },
      "sys/uuSubAppInstance/productInfo/get": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["Public"]
      },
      "getProductInfo": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["Public"]
      },
      "sys/uuSubAppInstance/productLogo/set": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities", "AsidExecutives"]
      },
      "sys/uuSubAppInstance/productLogo/get": {
        "sysStateList": ["created", "active", "restricted"],
        "profileList": ["Public"]
      },
      "getProductLogo": {
        "sysStateList": ["created", "active", "restricted"],
        "profileList": ["Public"]
      }
    }
  },
  "*": {
    "profileList": ["Authorities", "Executives", "Public"],
    "useCaseMap": {
      "sys/uuAppWorkspace/productInfo/set": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["Authorities", "Executives"]
      },
      "sys/uuAppWorkspace/productInfo/get": {
        "sysStateList": ["created", "active", "restricted", "readOnly", "suspended", "closed"],
        "profileList": ["Public"]
      },
      "getProductInfo": {
        "sysStateList": ["created", "active", "restricted", "readOnly", "suspended", "closed"],
        "profileList": ["Public"]
      },
      "sys/uuAppWorkspace/productLogo/set": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["Authorities", "Executives"]
      },
      "sys/uuAppWorkspace/productLogo/get": {
        "sysStateList": ["created", "active", "restricted", "readOnly", "suspended", "closed"],
        "profileList": ["Public"]
      },
      "getProductLogo": {
        "sysStateList": ["created", "active", "restricted", "readOnly", "suspended", "closed"],
        "profileList": ["Public"]
      }
    }
  }
}

      "sys/uuAppWorkspace/profile/update": {
        "sysStateList": ["restricted", "active"],
        "profileList": ["AsidAuthorities", "AsidExecutives"]
      },
      "sys/appClientToken/listKeys": {
        "sysStateList": ["created", "active", "restricted"],
        "profileList": ["Public"]
      },
      "sys/appClientToken/initKeys": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities"]
      },
      "sys/uuAppSecretStore/rotateMasterKey": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities"]
      },
      "sys/uuAppSecretStore/getStatus": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities"]
      }
    }

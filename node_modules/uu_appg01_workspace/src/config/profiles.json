{
  "{asid}": {
    "profileList": ["AsidAuthorities", "AsidExecutives", "AsidAuditors", "AsidLicenseOwner", "Public"],
    "useCaseMap": {
      "sys/uuSubAppInstance/init": {
        "sysStateList": ["created"],
        "profileList": ["AsidLicenseOwner", "Public"]
      },
      "sys/uuSubAppInstance/upgrade": {
        "sysStateList": ["created", "restricted"],
        "profileList": ["AsidLicenseOwner"]
      },
      "sys/uuSubAppInstance/upgradeCleanup": {
        "sysStateList": ["restricted", "active"],
        "profileList": ["AsidLicenseOwner", "AsidAuthorities"]
      },
      "sys/uuSubAppInstance/get": {
        "sysStateList": ["restricted", "active"],
        "profileList": ["AsidAuthorities", "AsidExecutives", "AsidAuditors"]
      },
      "sys/uuSubAppInstance/set": {
        "sysStateList": ["restricted", "active"],
        "profileList": ["AsidAuthorities", "AsidExecutives"]
      },
      "sys/uuSubAppInstance/profile/get": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities", "AsidExecutives", "AsidAuditors"]
      },
      "sys/uuSubAppInstance/profile/set": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities"]
      },
      "sys/uuSubAppInstance/profile/list": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities", "AsidExecutives", "AsidAuditors"]
      },
      "sys/uuSubAppInstance/permission/create": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidLicenseOwner","AsidAuthorities"]
      },
      "sys/uuSubAppInstance/permission/delete": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities"]
      },
      "sys/uuSubAppInstance/permission/list": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities", "AsidExecutives", "AsidAuditors"]
      },
      "sys/uuSubAppInstance/setActiveSysState": {
        "sysStateList": ["restricted"],
        "profileList": ["AsidAuthorities", "AsidExecutives"]

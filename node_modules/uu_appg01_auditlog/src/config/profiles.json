{
  "{tid}-{asid}": {
    "profileList": ["AsidAuthorities"],
    "useCaseMap": {
      "sys/logRecord/getTransferStatus": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities"]
      },
      "sys/logRecord/transfer": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["AsidAuthorities"]
      }
    }
  },
  "*": {
    "profileList": ["Authorities"],
    "useCaseMap": {
      "sys/logRecord/delete": {
        "sysStateList": ["active", "restricted"],
        "profileList": ["Authorities"]
      },
      "sys/logRecord/list": {
        "sysStateList": ["active", "restricted", "readOnly"],
        "profileList": ["Authorities"]
      }
    }
  }
}

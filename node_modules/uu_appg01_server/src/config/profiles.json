{
  "{asid}": {
    "profileList": ["AsidAuthorities", "Public"],
    "useCaseMap": {
      "sys/getHealth": {
        "sysStateList": ["created", "restricted", "active"],
        "profileList": ["Public"]
      },
      "sys/getUseCases": {
        "sysStateList": ["created", "restricted", "active"],
        "profileList": ["Public"]
      },
      "sys/getConfiguration": {
        "sysStateList": ["restricted", "active"],
        "profileList": ["AsidAuthorities"]
      },
      "sys/getEnvironment": {
        "sysStateList": ["restricted", "active"],
        "profileList": ["AsidAuthorities"]
      },
      "sys/getStatistics": {
        "sysStateList": ["restricted", "active"],
        "profileList": ["AsidAuthorities"]
      },
      "sys/resetStatistics": {
        "sysStateList": ["restricted", "active"],
        "profileList": ["AsidAuthorities"]
      },
      "sys/generateDiagnosticReport": {
        "sysStateList": ["restricted", "active"],
        "profileList": ["AsidAuthorities"]
      }
    }
  },
  "*": {
    "profileList": ["Authorities", "Public"],
    "useCaseMap": {
      "sys/getHealth": {
        "sysStateList": ["created", "restricted", "active", "readOnly", "suspended", "closed"],
        "profileList": ["Public"]
      },
      "sys/getUseCases": {
        "sysStateList": ["created", "restricted", "active", "readOnly", "suspended", "closed"],
        "profileList": ["Public"]
      },
      "sys/getStatistics": {
        "sysStateList": ["restricted", "active", "readOnly", "suspended", "closed"],
        "profileList": ["Authorities"]
      }
    }
  }
}
